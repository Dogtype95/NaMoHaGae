<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.kro.namohagae.mall.dao.ProductImageDao">
    <resultMap id="productImageResultMap" type="kr.kro.namohagae.mall.entity.ProductImage">
        <id property="productNo" column="product_no" />
        <result property="productImageNo" column="product_image_no" />
        <result property="productImageFilename" column="product_image_filename" />
    </resultMap>

    <update id="save">
        insert all
        <foreach collection="list" item="item" separator=" ">
            into PRODUCT_IMAGE values(#{item.productNo, jdbcType=INTEGER}, #{item.productImageNo, jdbcType=VARCHAR}, #{item.productImageFilename, jdbcType=VARCHAR})
        </foreach>
        select * from dual
    </update>
</mapper>