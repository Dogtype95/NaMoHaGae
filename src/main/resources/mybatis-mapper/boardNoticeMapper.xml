<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.kro.namohagae.board.dao.BoardNoticeDao">

    <insert id="save">
        INSERT INTO BOARD_NOTICE
    values (BOARD_NOTICE_SEQ_BOARD_NOTICE_NO.nextval,#{boardNoticeWriter}, #{boardNoticeTitle},#{boardNoticeContent}, #{boardNoticeWriteDate}, 0)
    </insert>

    <select id="findAll" resultType="java.util.List" >
        select *
        from BOARD_NOTICE
    </select>

</mapper>

